{
	"name": "plugin",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint"
	},
	"dependencies": {
		"@types/styled-components": "^5.1.24",
		"babel-plugin-styled-components": "^2.0.7",
		"next": "12.1.0",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"socket.io": "^4.4.1",
		"socket.io-client": "^4.4.1",
		"styled-components": "^5.3.3"
	},
	"devDependencies": {
		"@types/node": "17.0.21",
		"@types/react": "17.0.41",
		"eslint": "8.11.0",
		"eslint-config-next": "12.1.0",
		"typescript": "4.6.2"
	},
	"babel": {
		"env": {
			"development": {
				"presets": [
					"next/babel"
				],
				"plugins": [
					[
						"styled-components",
						{
							"ssr": true,
							"displayName": true
						}
					]
				]
			},
			"production": {
				"presets": [
					"next/babel"
				],
				"plugins": [
					[
						"styled-components",
						{
							"ssr": true,
							"displayName": false
						}
					]
				]
			}
		}
	}
}
